pipeline{
  agent none
  stages{
    stage('complie'){
      agent any
      steps{
        sh 'compile'
       }
     }
     stage('Code quality'){
      agent any
      steps{
        sh 'echo Sonarqube Code Quality Check Done'
       }
     }
     stage('test'){
      agent any
      steps{
        sh 'mvn test'
        }
      }
    stage('package'){
      agent any
      steps{
        sh 'mvn package'
      }
    }
  }
}
  
